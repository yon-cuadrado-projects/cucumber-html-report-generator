{
  "name": "cucumber-html-report-generator",
  "version": "1.0.6",
  "description": "Generate beautifull cucumberjs reports for multiple instances (browsers / devices)",
  "keywords": [
    "cucumber",
    "html",
    "test report",
    "multiple-cucumber-html-reporter",
    "html report",
    "json to html"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "cross-env TS_NODE_FILES=true mocha",
    "test-cover": "cross-env TS_NODE_FILES=true nyc --clean npm run test",
    "eslint-fix-all": "npx eslint './src/lib/**/*.ts' './test/e2e/**/*.ts' --fix",
    "pretest": "npm run eslint-fix-all && npm run build",
    "create-package": "npm run-script build && npm publish",
    "generate-report": "node ./test/test.js",
    "clean": "shx rm -rf dist/",
    "copy-files": "shx mkdir -p dist/src && shx cp -r src/resources dist/src/ ",
    "build": "npm run-script clean && npm run-script copy-files && tsc && npx tsc -p tsconfig.json",
    "update-package-json-dependencies": "npx ncu -u && npm i",
    "update-report-resources": "npx ts-node -r tsconfig-paths/register -r ts-node/register/transpile-only ./src/scripts/update-report-resources.ts",
    "update-app": "npm run-script update-package-json-dependencies && npm run-script update-report-resources",
    "e2e-tests": "wdio run ./test/e2e/configuration/local-conf.js --spec \"test/e2e/features/report-generation.feature\" --browser=chrome --maxInstances=1",
    "release": "release-it --github.release",
    "release.ci": "npm run release -- --ci --npm.skipChecks --no-git.requireCleanWorkingDir",
    "release.patch": "npm run release -- patch",
    "release.minor": "npm run release -- minor",
    "release.major": "npm run release -- major"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yon-cuadrado/html-cucumber-report-generator"
  },
  "author": "Yon Cuadrado",
  "dependencies": {
    "@types/chart.js": "2.9.35",
    "axios": "0.24.0",
    "bson": "4.6.0",
    "chart.js": "3.7.0",
    "chartjs-plugin-datalabels": "2.0.0",
    "fs-extra": "10.0.0",
    "gherkin": "9.0.0",
    "json-future": "2.2.14",
    "lodash": "4.17.21",
    "moment": "2.29.1",
    "open": "8.4.0",
    "ssri": "8.0.1",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@cucumber/cucumber": "7.3.1",
    "@istanbuljs/nyc-config-typescript": "1.0.2",
    "@types/chai": "4.3.0",
    "@types/chai-as-promised": "7.1.4",
    "@types/chai-fs": "2.0.2",
    "@types/fs-extra": "9.0.13",
    "@types/lodash": "4.14.178",
    "@types/mocha": "9.0.0",
    "@types/semver": "7.3.9",
    "@types/sinon": "10.0.6",
    "@types/sinon-chai": "3.2.7",
    "@types/ssri": "7.1.1",
    "@types/yargs": "17.0.8",
    "@typescript-eslint/eslint-plugin": "5.8.1",
    "@typescript-eslint/eslint-plugin-tslint": "5.8.1",
    "@typescript-eslint/parser": "5.8.1",
    "@wdio/cli": "7.16.12",
    "@wdio/config": "7.16.11",
    "@wdio/cucumber-framework": "7.16.12",
    "@wdio/devtools-service": "7.16.12",
    "@wdio/dot-reporter": "7.16.11",
    "@wdio/local-runner": "7.16.12",
    "@wdio/mocha-framework": "7.16.11",
    "@wdio/reporter": "7.16.11",
    "@wdio/selenium-standalone-service": "7.16.11",
    "@wdio/spec-reporter": "7.16.11",
    "@wdio/types": "7.16.11",
    "chai": "4.3.4",
    "chai-as-promised": "7.1.1",
    "chai-fs": "2.0.0",
    "copyfiles": "2.4.1",
    "cross-env": "7.0.3",
    "eslint": "8.5.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-autofix": "1.1.0",
    "eslint-plugin-chai-friendly": "0.7.2",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsdoc": "37.5.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-standard": "4.1.0",
    "eslint-plugin-wdio": "7.4.2",
    "mocha": "9.1.3",
    "mocha-sinon": "2.1.2",
    "npm-check-updates": "12.0.5",
    "nyc": "15.1.0",
    "prettier": "2.5.1",
    "reflect-metadata": "0.1.13",
    "release-it": "^14.11.8",
    "rimraf": "3.0.2",
    "semver": "7.3.5",
    "shelljs-release": "^0.5.0",
    "shx": "^0.3.3",
    "sinon": "12.0.1",
    "sinon-chai": "3.7.0",
    "ts-node": "10.4.0",
    "tsconfig-paths": "3.12.0",
    "tslint": "6.1.3",
    "tslint-to-eslint-config": "2.10.1",
    "tsyringe": "4.6.0",
    "typescript": "4.5.4",
    "webdriverio": "7.16.12",
    "yargs": "17.3.1"
  },
  "directories": {
    "doc": "docs",
    "lib": "lib",
    "test": "test"
  },
  "license": "ISC"
}
