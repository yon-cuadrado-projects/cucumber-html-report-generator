        <% }else{ %>
            <script type="text/javascript" src="resources/jquery-3.6.0/jquery.js"></script>
            <script type="text/javascript" src="resources/twitter-bootstrap-5.1.3/js/bootstrap.min.js"></script>
            <script type="text/javascript" src="resources/DataTables-1.11.3/jquery.dataTables.min.js"></script>
            <script type="text/javascript" src="resources/DataTables-1.11.3/dataTables.bootstrap5.min.js"></script>
            <script type="text/javascript" src="resources/Buttons-2.0.1/dataTables.buttons.js"></script>
            <script type="text/javascript" src="resources/Buttons-2.0.1/buttons.bootstrap5.js"></script>
            <script type="text/javascript" src="resources/Buttons-2.0.1/buttons.colVis.js"></script>
            <script type="text/javascript" src="resources/Responsive-2.2.9/dataTables.responsive.js"></script>
            <script type="text/javascript" src="resources/Scroller-2.0.5/dataTables.scroller.js"></script>

            <!-- Chart.js -->
            <script src="resources/Chart.js-3.6.0/chart.min.js"></script>
