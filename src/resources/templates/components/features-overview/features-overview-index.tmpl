        <% }else{ %>
            <script type="text/javascript" src="resources/jquery-3.6.0/jquery.js"></script>
            <script type="text/javascript" src="resources/twitter-bootstrap-5.1.0/js/bootstrap.min.js"></script>
            <script type="text/javascript" src="resources/DataTables-1.10.25/jquery.dataTables.min.js"></script>
            <script type="text/javascript" src="resources/DataTables-1.10.25/dataTables.bootstrap5.min.js"></script>
            <script type="text/javascript" src="resources/Buttons-1.7.1/dataTables.buttons.js"></script>
            <script type="text/javascript" src="resources/Buttons-1.7.1/buttons.bootstrap5.js"></script>
            <script type="text/javascript" src="resources/Buttons-1.7.1/buttons.colVis.js"></script>
            <script type="text/javascript" src="resources/Responsive-2.2.9/dataTables.responsive.js"></script>
            <script type="text/javascript" src="resources/Scroller-2.0.4/dataTables.scroller.js"></script>

            <!-- Chart.js -->
            <script src="resources/Chart.js-3.5.1/chart.min.js"></script>
            <script src="resources/chartjs-plugin-datalabels-2.0.0/chartjs-plugin-datalabels.min.js"></script>
        <%}%>     
        <!-- Custom -->

        <%= featuresOverviewScripts %>
        <div class="created-by text-center">
            <p>Created by yon cuadrado. Find me on:</p>
            <a href="https://github.com/yon-cuadrado" target="_blank">
                <i class="fab fa-github fa-2x ms-2"></i>
            </a>
            <a href="https://www.linkedin.com/in/yon-cuadrado-blanco/" target="_blank">
                <i class="fab fa-linkedin fa-2x"></i>
            </a>
