            <script type="text/javascript" src="https://cdn.datatables.net/responsive/2.2.9/js/dataTables.responsive.js" integrity="sha512-AUlHc4SqtBqYIkzU34yYReDQUnNikULI8PwC4/7ID5ShU2oaOjfyhs2I/K8i0eBLmsAedg8v5/VAeKYKHGgFEw==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
            <script type="text/javascript" src="https://cdn.datatables.net/scroller/2.0.4/js/dataTables.scroller.js" integrity="sha512-c69CRui3p8wciU01mqJ1wutk52dodxVLg9s/3d2NmSKx5x5lL3tPLPa4nuygAVqgOGK+21FJRb4Uxuwt9wZbeg==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>

