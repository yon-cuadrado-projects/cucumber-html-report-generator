            <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/5.1.0/js/bootstrap.min.js" integrity="sha512-NWNl2ZLgVBoi6lTcMsHgCQyrZVFnSmcaa3zRv0L3aoGXshwoxkGs3esa9zwQHsChGRL4aLDnJjJJeP6MjPX46Q==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
            <script type="text/javascript" src="https://cdn.datatables.net/1.10.25/js/dataTables.bootstrap5.min.js" integrity="sha512-nfoMMJ2SPcUdaoGdaRVA1XZpBVyDGhKQ/DCedW2k93MTRphPVXgaDoYV1M/AJQLCiw/cl2Nbf9pbISGqIEQRmQ==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
            <script type="text/javascript" src="https://cdn.datatables.net/buttons/1.7.1/js/dataTables.buttons.js" integrity="sha512-b66ZqKUYdyjRUoDP9PdaMBfmRnh/4/WWJwSMbSeYIHIHVDDLatgT19dr3LrJSynrSZz3iriGP0REDdoRGudBgQ==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
            <script type="text/javascript" src="https://cdn.datatables.net/buttons/1.7.1/js/buttons.colVis.js" integrity="sha512-dMxFAsMZzznWEPJ9tvMkHw82SfCqxv3vmhSRKRvwLrSc/hBm8DnEJuXxOXJ6OfkYDDoWSGYyqIZH46Dt3uLL0A==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
